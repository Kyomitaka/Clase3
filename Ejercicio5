package com.mycompany.clase3e5;

public class Clase3E5 {

    public static void main(String[] args) {
        System.out.println("NÃºmero Cuadrado");
        System.out.println("\n");
        
        for (int i = 20; i <= 30; i++) {
            int cuadrado = i * i;
            System.out.println(i + " " + cuadrado);
    }
        
    }
        
}
